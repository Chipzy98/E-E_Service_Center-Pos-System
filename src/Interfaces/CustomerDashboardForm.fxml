<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #b2bec3;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.CustomerDashboardForm">
   <children>
      <Label alignment="CENTER" layoutY="-1.0" prefHeight="73.0" prefWidth="600.0" style="-fx-background-color: #2ecc71;" text="Customer Dashboard" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="Calibri Bold" size="40.0" />
         </font>
      </Label>
      <Button fx:id="logoutbtn" layoutX="14.0" layoutY="23.0" mnemonicParsing="false" onAction="#LogoutbuttonOnAction" style="-fx-background-color: #e74c3c;" text="Log Out" textFill="WHITE">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
      <Button fx:id="porderbtn" layoutX="48.0" layoutY="120.0" mnemonicParsing="false" onAction="#PorderbuttonOnAction" prefHeight="80.0" prefWidth="212.0" style="-fx-background-color: #B8D8E3;" text="Place Order">
         <font>
            <Font name="Candara Bold" size="24.0" />
         </font>
      </Button>
      <Button fx:id="oreportbtn" layoutX="48.0" layoutY="248.0" mnemonicParsing="false" onAction="#OreportbuttonOnAction" prefHeight="80.0" prefWidth="212.0" style="-fx-background-color: #9DB0CE;" text="Order Report">
         <font>
            <Font name="Candara Bold" size="24.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
